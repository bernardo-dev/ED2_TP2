{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ordena",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/Debug/ordena",
      "args": ["1", "10", "3", "-P"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/bin/Debug",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set disassembly-flavor intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ],
  "compounds": []
}